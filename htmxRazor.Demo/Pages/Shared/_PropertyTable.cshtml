@model List<htmxRazor.Demo.Models.ComponentProperty>

<section class="docs-component-section">
    <h2>Properties</h2>
    <div class="docs-property-table">
        <table>
            <thead>
                <tr>
                    <th>Property</th>
                    <th>Type</th>
                    <th>Default</th>
                    <th>Description</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var prop in Model)
                {
                    <tr>
                        <td><code>@prop.Name</code></td>
                        <td><code>@prop.Type</code></td>
                        <td><code>@prop.Default</code></td>
                        <td>@prop.Description</td>
                    </tr>
                }
            </tbody>
        </table>
    </div>
</section>
